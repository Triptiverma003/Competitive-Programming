#include<bits/stdc++.h>
using namespace std;

class Complex{
    public:
    int real;
    int imag;
    
    Complex() {
        real = imag = -1;
    }
    Complex(int r , int i):real(r),imag(i){};
    Complex operator+(const Complex &B){
        Complex temp;
        temp.real = this->real+B.real;
        temp.imag = this->imag+B.imag;
        return temp;
    }
    void print() {
        cout << real << " + " << imag << "i" << endl;
    }
};
int main(){
    Complex A(3,3);
    A.print();
    Complex B(3,3);
    B.print();
    Complex C = A + B;
    C.print();
    return 0;
}
